@model IEnumerable<MATJParking.Web.Models.VehicleType>
@{
    ViewBag.Title = "Home Page";
}


<div class="jumbotron">
    <h1>MATJ Parking</h1>
</div>

<div class="row">
    <div class="col-md-4">
        
            @using (Html.BeginForm())
{
                @Html.AntiForgeryToken()
          <div class="main">      
            <div class="form-group">
                <label for="usr">Registration number :</label>
                <input type="text" class="form-control" id="number" name="registrationNumber">
            </div>
        </div>
        <div class="pageButtons">
            <button type="submit" class="btn btn-success" formaction="~/Vehicle/CheckIn">Check in</button>
            <button type="submit" class="btn btn-primary" formaction="~/Vehicle/checkout">Check out</button>
            <button type="submit" class="btn btn-default" formaction="~/Vehicle/SearchCar">Search</button>
        </div>
            }
    </div>
</div>